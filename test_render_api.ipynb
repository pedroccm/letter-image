   "source": [
    "import requests\n",
    "import time\n",
    "\n",
    "# Teste da API do Render - bem simples\n",
    "BASE_URL = \"https://letter-image.onrender.com\"\n",
    "team_name = \"flamengo\"\n",
    "\n",
    "print(f\"üöÄ Testando API Render para: {team_name}\")\n",
    "print(f\"üîó URL: {BASE_URL}\")\n",
    "\n",
    "# Primeiro teste: verificar se API est√° viva\n",
    "print(\"\\n1Ô∏è‚É£ Testando conex√£o...\")\n",
    "try:\n",
    "    response = requests.get(f\"{BASE_URL}/\", timeout=10)\n",
    "    print(f\"‚úÖ API respondeu: {response.status_code} - {response.json()}\")\n",
    "except Exception as e:\n",
    "    print(f\"‚ùå Erro na conex√£o: {e}\")\n",
    "    exit()\n",
    "\n",
    "# Segundo teste: gerar 1 background apenas\n",
    "print(\"\\n2Ô∏è‚É£ Gerando 1 background...\")\n",
    "data = {\n",
    "    \"team_name\": team_name,\n",
    "    \"count\": 1,  # Apenas 1 para ser mais r√°pido\n",
    "    \"size\": \"1024x1536\",\n",
    "    \"quality\": \"medium\"\n",
    "}\n",
    "\n",
    "start_time = time.time()\n",
    "try:\n",
    "    response = requests.post(\n",
    "        f\"{BASE_URL}/generate-team-backgrounds\", \n",
    "        json=data,\n",
    "        timeout=300  # 5 minutos de timeout\n",
    "    )\n",
    "    \n",
    "    elapsed = time.time() - start_time\n",
    "    print(f\"‚è±Ô∏è Tempo decorrido: {elapsed:.1f}s\")\n",
    "    print(f\"üìä Status: {response.status_code}\")\n",
    "    \n",
    "    if response.status_code == 200:\n",
    "        result = response.json()\n",
    "        print(f\"‚úÖ Sucesso! {result['count']} imagem(ns) gerada(s)\")\n",
    "        print(f\"üèÜ Time: {result['team_name']}\")\n",
    "        print(\"\\nüì∏ URLs:\")\n",
    "        \n",
    "        for i, url in enumerate(result['urls'], 1):\n",
    "            print(f\"  {i}. {url}\")\n",
    "    else:\n",
    "        print(f\"‚ùå Erro: {response.status_code}\")\n",
    "        print(f\"Detalhes: {response.text}\")\n",
    "        \n",
    "except requests.Timeout:\n",
    "    print(f\"‚è∞ Timeout ap√≥s {time.time() - start_time:.1f}s\")\n",
    "except Exception as e:\n",
    "    print(f\"‚ùå Erro: {e}\")\n",
    "\n",
    "print(\"\\nüèÅ Teste conclu√≠do\")"
   ]
  }
